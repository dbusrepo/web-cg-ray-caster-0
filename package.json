  "scripts": {
    "v": "node -v",
    "eslint_v": "eslint -v",
    "type-check": "tsc --noemit",
    "install-dependencies": "npm i",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prettify": "prettier --config .prettierrc src/**/*.{ts,tsx} --write",
    "prettify-watch": "onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}",
    "onchange:wasm": "onchange -v -i 'src/engine/wasmEngine/wasm/compile_asc.mjs' 'src/engine/wasmEngine/wasm/wasmConfig.mjs' 'src/engine/wasmEngine/wasm/makefile' 'src/engine/wasmEngine/wasm/asconfig.json' 'src/engine/wasmEngine/wasm/src/**/*.ts' -- make --no-print-directory -C src/engine/wasmEngine/wasm ",
    "onchange:images": "onchange -v -i './assets/makefile' './assets/images/genImagesList.mjs' './assets/images/images.res' -- make --no-print-directory -C ./assets images",
    "onchange:strings": "onchange -v -i './assets/makefile' './assets/strings/genStringsList.mjs' './assets/strings/strings.res' -- make --no-print-directory -C ./assets strings",
    "onchange:assets": "run-p --race onchange:images onchange:strings",
    "watch": "webpack --watch",
    "build:assets": "cd ./assets && make all",
    "build:asc": "cd src/engine/wasmEngine/wasm && make build_asc",
    "build:wat": "cd src/engine/wasmEngine/wasm && make build_wat",
    "build:wasm": "npm run build:asc && npm run build:wat",
    "webpack": "webpack --config ./webpack.config.ts",
    "build_proj": "npm run build:assets && npm run build:wasm && webpack",
    "serve": "webpack-dev-server",
    "launch": "npm run cleandist && npm run build_proj && npm run serve",
    "prod": "webpack --production",
    "clean": "rimraf dist/ && (cd ./assets/ && make clean) && (cd src/engine/wasmEngine/wasm && make clean)",
    "cleandist": "rimraf dist/",
    "ascv": "asc --version",
    "tscv": "tsc --version",
    "build": "npm run build_proj",
    "start": "run-p --race launch onchange:wasm onchange:assets",
    "build_i": "npm run install-dependencies && npm run build_proj",
    "start_i": "npm run install-dependencies && run-p --race launch onchange:wasm onchange:assets"
  },
