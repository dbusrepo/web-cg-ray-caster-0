{
  "options": {
    "verbose": true,
    "pedantic": false,
    "measure": true,
    "traceResolution": true,

    "sourceMap": true,
    "noAssert": false,

    "initialMemory": 64,
    "memoryBase": 4194304,
    "maximumMemory": 1000,

    "importMemory": true,
    "sharedMemory": true,
    "noExportMemory": true,
    "zeroFilledMemory": true,

    "importTable": false,
    "exportTable": false,

    "stats": true,

    "enable": ["sign-extension", "nontrapping-f2i", "threads", "simd", "relaxed-simd", "bulk-memory", "reference-types"],
    "disable": ["mutable-globals", "gc"],

    "runtime": "stub",
    "exportRuntime": false
  },
  "targets": {
    "release-speed": {
      "optimizeLevel": 3,
      "shrinkLevel": 1,
      "converge": true
    },
    "release-size": {
      "optimizeLevel": 1,
      "shrinkLevel": 2,
      "converge": true
    },
    "debug": {
      "debug": true
    }
  }
}
